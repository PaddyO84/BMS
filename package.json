{
  "name": "clear-path",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "mkdirp public/assets && copyfiles -u 1 node_modules/sql.js/dist/sql-wasm.wasm public/assets && vite build",
    "clean": "rimraf dist android/app/src/main/assets/public",
    "build:android": "npm run clean && npm run build && npx cap copy android",
    "preview": "vite preview"
  },
  "dependencies": {
    "@capacitor-community/sqlite": "^5.7.2",
    "@capacitor/android": "^5.7.4",
    "@capacitor/app": "^5.0.7",
    "@capacitor/camera": "^5.0.9",
    "@capacitor/core": "^5.7.4",
    "@capacitor/filesystem": "^5.2.1",
    "@capacitor/share": "^5.0.7",
    "jspdf": "^2.5.1",
    "jspdf-autotable": "^3.5.28",
    "lucide-react": "^0.395.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.24.0"
  },
  "devDependencies": {
    "@capacitor/cli": "^5.7.4",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.14",
    "copyfiles": "^2.4.1",
    "mkdirp": "^3.0.1",
