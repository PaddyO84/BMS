{
  "name": "clear-path",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "mkdirp public/assets && copyfiles -u 1 node_modules/sql.js/dist/sql-wasm.wasm public/assets && vite",
    "build": "mkdirp public/assets && copyfiles -u 1 node_modules/sql.js/dist/sql-wasm.wasm public/assets && vite build",
    "clean": "rimraf dist android/app/src/main/assets/public",
    "build:android": "npm run clean && npm run build && npx cap copy android",
    "preview": "vite preview"
  },
  "dependencies": {
    "@capacitor-community/sqlite": "^7.0.1",
    "@capacitor/android": "^7.4.3",
    "@capacitor/app": "^7.1.0",
    "@capacitor/camera": "^7.0.2",
    "@capacitor/core": "^7.4.3",
    "@capacitor/filesystem": "^7.1.4",
    "@capacitor/share": "^7.0.2",
    "jspdf": "^3.0.3",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.395.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.24.0"
  },
  "devDependencies": {
    "@capacitor/cli": "^7.4.3",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.14",
    "copyfiles": "^2.4.1",
    "mkdirp": "^3.0.1",
    "postcss": "^8.4.27",
    "rimraf": "^6.0.1",
    "tailwindcss": "^3.3.3",
    "vite": "^7.1.7"
  }
}
